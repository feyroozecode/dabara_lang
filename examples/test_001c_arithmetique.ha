# TEST 001c: Opérations arithmétiques
# Ce test montre les opérations mathématiques de base :
# - Addition (+)
# - Soustraction (-)
# - Multiplication (*)
# - Division (/)

fara
    naɗa lambar1 = 20
    naɗa lambar2 = 8
    
    rubuta "=== Test Opérations Arithmétiques ==="
    rubuta "Lambar1: "
    rubuta lambar1
    rubuta "Lambar2: "
    rubuta lambar2
    
    # Addition
    naɗa jimla = lambar1 + lambar2
    rubuta "Jimla (+): "
    rubuta jimla
    
    # Soustraction
    naɗa bambanci = lambar1 - lambar2
    rubuta "Bambanci (-): "
    rubuta bambanci
    
    # Multiplication
    naɗa ninka_sakamako = lambar1 * lambar2
    rubuta "Ninka: "
    rubuta ninka_sakamako
    
    # Division
    naɗa raba_sakamako = lambar1 / lambar2
    rubuta "Raba: "
    rubuta raba_sakamako
    
    # Opération complexe
    naɗa lissafi_hadadden = lambar1 * 3 + lambar2 / 2 - 5
    rubuta "Lissafi hadadden: "
    rubuta lissafi_hadadden
    
    rubuta "=== Gama test lissafi! ==="
