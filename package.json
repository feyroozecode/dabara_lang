{
  "name": "dabara-docs",
  "version": "1.0.0",
  "description": "Documentation officielle du langage de programmation Dabara",
  "scripts": {
    "build": "cd docs && mdbook build",
    "serve": "cd docs && mdbook serve --open",
    "deploy:vercel": "cd docs && ./build-and-deploy.sh vercel",
    "deploy:netlify": "cd docs && ./build-and-deploy.sh netlify",
    "deploy:github": "cd docs && ./build-and-deploy.sh github",
    "clean": "rm -rf docs/book",
    "install:mdbook": "cargo install mdbook",
    "test": "cd docs && mdbook test"
  },
  "keywords": [
    "dabara",
    "hausa",
    "programming-language",
    "documentation",
    "african-languages",
    "mdbook"
  ],
  "author": "Ahmad",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ahmad-dabara/dabara.git"
  },
  "homepage": "https://dabara-docs.vercel.app",
  "bugs": {
    "url": "https://github.com/ahmad-dabara/dabara/issues"
  },
  "devDependencies": {
    "vercel": "^32.5.0",
    "netlify-cli": "^17.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}